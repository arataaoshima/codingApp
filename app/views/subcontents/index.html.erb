<p id="notice"><%= notice %></p>
<div class="text-center">
<h1>Subcontents</h1>
</div>
<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Type</th>
      <th>Article</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @subcontents.each do |subcontent| %>
      <tr>
        <td><%= subcontent.name %></td>
        <td><%= subcontent.subcontent_type %></td>
        <td><%= subcontent.article_id %></td>
        <td><%= link_to 'Show', subcontent %></td>
        <td><%= link_to 'Edit', edit_subcontent_path(subcontent) %></td>
        <td><%= link_to 'Destroy', subcontent, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Subcontent', new_subcontent_path %>
